<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cosplay.kotlin.hw">

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".APP"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.LayoutAct" />
        <activity android:name=".ui.activity.Handler2ThreadAct" />
        <activity android:name=".ui.activity.ViewPagerAct" />
        <activity android:name=".ui.activity.Main2Activity" />
        <activity android:name=".ui.activity.NetAct" />
        <activity android:name=".ui.activity.FragmentAct" />
    </application>

</manifest>