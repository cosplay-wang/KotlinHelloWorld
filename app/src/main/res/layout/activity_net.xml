<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cosplay.kotlin.hw.ui.activity.NetAct">
    <TextView
        android:id="@+id/tv_click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/FragmentText"
        android:text="@string/click_net"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />
    <TextView
        android:id="@+id/tv_click_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/FragmentText"
        android:text="@string/click_net"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/tv_click"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
       />
    <TextView
        android:id="@+id/tv_click_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/FragmentText"
        android:text="@string/click_net"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/tv_click_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

</android.support.constraint.ConstraintLayout>
